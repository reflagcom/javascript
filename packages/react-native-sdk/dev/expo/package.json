{
  "name": "reflag-react-sdk-expo",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "build:deps": "rm -rf ../../browser-sdk/dist ../../react-sdk/dist ../../react-native-sdk/dist && yarn workspace @reflag/browser-sdk build && yarn workspace @reflag/react-sdk build && yarn workspace @reflag/react-native-sdk build",
    "prestart": "yarn build:deps",
    "preandroid": "yarn build:deps",
    "preios": "yarn build:deps",
    "preweb": "yarn build:deps",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@reflag/react-native-sdk": "workspace:*",
    "expo": "~54.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.0"
  }
}
